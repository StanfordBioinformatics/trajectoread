{
    "applets": {
        "bcl2fastq": {
            "dxid": "", 
            "external_rscs": [
                {
                    "name": "bcl2fastq", 
                    "version": "1.8.4"
                }, 
                {
                    "name": "bcl2fastq2", 
                    "version": "2.17.1.14"
                }
            ], 
            "internal_rscs": {
                "python_packages": [
                    "scgpm_lims"
                ], 
                "scripts": [
                    "create_sample_sheet.py", 
                    "calculate_use_bases_mask.py"
                ]
            }
        }, 
        "generate_qc_report": {
            "dxid": "", 
            "external_rscs": [
                {
                    "name": "pdf_reports", 
                    "version": "1.0"
                }
            ], 
            "internal_rscs": {}
        }, 
        "qc_controller": {
            "dxid": "", 
            "external_rscs": [
                {
                    "name": "pdf_reports", 
                    "version": "1.0"
                }
            ], 
            "internal_rscs": {}
        }, 
        "qc_sample": {
            "dxid": "", 
            "external_rscs": [
                {
                    "name": "picard_tools", 
                    "version": "1.107"
                }, 
                {
                    "name": "fastqc", 
                    "version": "0.10.1"
                }
            ], 
            "internal_rscs": {
                "c_programs": [
                    "bwa_mismatches", 
                    "collect_uniqueness_metrics"
                ]
            }
        }, 
        "release_lane": {
            "dxid": "", 
            "external_rscs": [], 
            "internal_rscs": {
                "python_packages": [
                    "scgpm_lims"
                ]
            }
        }
    }, 
    "date_created": "", 
    "dx_OS": "", 
    "edit_version": 0, 
    "stages": {
        "0": {
            "dxid": "", 
            "executable": "bcl2fastq",
            "type": "stand-alone", 
            "folder": "stage0_bcl2fastq", 
            "input": {
                "lane_data_tar": "$dnanexus_link-lane_data_tar_id", 
                "metadata_tar": "$dnanexus_link-metadata_tar_id", 
                "output_folder": "/stage0_bcl2fastq", 
                "record_id": "$record_id",
                "test_mode": "$test_mode",
                "barcode_mismatches": "$barcode_mismatches"
            }, 
            "linked_input": {}, 
            "name": "Demultiplex and bcl2fastq", 
            "output": [
                "fastqs", 
                "tools_used"
            ]
        }, 
        "1": {
            "dxid": "", 
            "executable": "qc_controller", 
            "type": "controller",
            "worker_name": "qc_sample",
            "folder": "stage1_qc", 
            "input": {
                "record_id": "$record_id",
                "worker_id": "", 
                "worker_project": "",
                "output_folder": "/stage1_qc"
            }, 
            "linked_input": {
                "fastqs": {
                    "field": "outputField", 
                    "name": "fastqs", 
                    "stage": "0"
                }
            }, 
            "name": "QC Report", 
            "output": [
                "tools_used", 
                "mismatch_metrics", 
                "qc_stats_jsons", 
                "fastqc_reports", 
                "alignment_summary_metrics", 
                "insert_size_metrics"
            ]
        }, 
        "2": {
            "dxid": "", 
            "executable": "generate_qc_report",
            "type": "stand-alone", 
            "folder": "stage2_qc_report", 
            "input": {
                "record_id": "$record_id",
                "output_folder": "/stage2_qc_report",
                "interop_tar": "$dnanexus_link-interop_tar_id"
            }, 
            "linked_input": {
                "fastqs": {
                    "field": "outputField", 
                    "name": "fastqs", 
                    "stage": "0"
                }, 
                "mismatch_metrics": {
                    "field": "outputField", 
                    "name": "mismatch_metrics", 
                    "stage": "1"
                }, 
                "qc_stats_jsons": {
                    "field": "outputField", 
                    "name": "qc_stats_jsons", 
                    "stage": "1"
                }, 
                "tools_used": [
                    {
                        "executable": "bcl2fastq", 
                        "field": "outputField", 
                        "name": "tools_used", 
                        "stage": "0"
                    }, 
                    {
                        "executable": "qc_controller", 
                        "field": "outputField", 
                        "name": "tools_used", 
                        "stage": "1"
                    }
                ]
            }, 
            "name": "Generate QC Report", 
            "output": [
                "qc_pdf_report"
            ]
        }, 
        "3": {
            "dxid": "", 
            "executable": "release_lane", 
            "type": "stand-alone",
            "folder": "", 
            "input": {
                      "record_id": "$record_id",
                      "develop": "$develop"
            }, 
            "linked_input": {
                "qc_pdf_report": {
                    "field": "outputField", 
                    "name": "qc_pdf_report", 
                    "stage": "2"
                }
            }, 
            "name": "Release Lane Project", 
            "tools_used": []
        }
    }, 
    "name": "SCGPM_bcl2fastq_qc_release",
    "status": "production",
    "workflow_dxid": "", 
    "workflow_project_dxid": ""
}